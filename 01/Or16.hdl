/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
//notaï¼Œreverse a or b
//repeatedly 
Nand(a=a[0], b=a[0], out=nota[0]);
Nand(a=b[0], b=b[0], out=notb[0]);
Nand(a=nota[0], b=notb[0], out=out[0]);


}