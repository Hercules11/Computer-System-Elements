/**
 * The main class of the Snake game. // 先模仿着来
 */
class Main {

    /** Initializes a Snake game and starts running it. */
    function void main() {
        var SnakeGame game;
        let game = SnakeGame.new();
        do game.run();
        do game.dispose();
        return;
    }
}

